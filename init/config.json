{
    "name": "testxapp-python",
    "version": "1.0.1",
    "containers": [
        {
            "name": "testxapp",
            "image": {
                "registry": "registry.hub.docker.com",
                "name": "109010/testxapp",
                "tag": "1.0.1"
            }
        }
    ],
    "messaging": {
        "ports": [
            {
                "name": "http",
                "container": "testxapp",
                "port": 8080,
                "description": "http service"
            },
            {
                "name": "rmrroute",
                "container": "testxapp",
                "port": 4561,
                "description": "rmr route port for testxapp xapp"
            },
            {
                "name": "rmrdata",
                "container": "testxapp",
                "port": 4560,
                "rxMessages": ["A1_POLICY_REQ_MSG_TYPE"],
                "txMessages": ["A1_POLICY_RESP_MSG_TYPE", "A1_POLICY_QUERY_MSG_TYPE"],
                "policies": [1],
                "description": "rmr data port for testxap"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "txMessages": [
            "A1_POLICY_RESP_MSG_TYPE", "A1_POLICY_QUERY_MSG_TYPE"
        ],
        "rxMessages": [
            "A1_POLICY_REQ_MSG_TYPE"
        ],
    "policies": [1]
    }
}